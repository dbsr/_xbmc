<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<addon id="plugin.video.filestube" name="filestube" provider-name="dbsr" version="0.1">
    <requires>
        <import addon="xbmc.python" version="2.1.0" />
        <import addon="script.module._addon" version="0.0.2" />
        <import addon="script.module.urlresolver" version="2.1.0" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>Filestube search and play from within xbmc. Real-Debrid account required</summary>
        <description>This addon uses the newznab api to browse media and pneum</description>
        <platform>all</platform>
    </extension>
</addon>
